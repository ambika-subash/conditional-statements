#!/bin/sh
#Script to check if 2 user-inputed numbers are equal to, greater or lesser than each other
#(if-else nested within an if statement)
echo "Enter a"
read a
echo "Enter b"
read b
if [ $a == $b ];
then
     echo "a is equal to b"
else
     if [ $a -lt $b ];
     then
         echo "a is less than b"
     else
        echo "a is greater than b"
     fi
fi
